@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/zwaba/Ztoggle4>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-ztoggle4.html> ;
        modgui:stylesheet <modgui/stylesheet-ztoggle4.css> ;
        modgui:screenshot <modgui/screenshot-ztoggle4.png> ;
        modgui:thumbnail <modgui/thumbnail-ztoggle4.png> ;
        modgui:brand "zwabo" ;
        modgui:label "Ztoggle4" ;
        modgui:model "boxy-small" ;
        modgui:panel "1-footswitch" ;
        modgui:color "cream" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Cycle" ;
            lv2:name "Cycle" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Killswitch" ;
            lv2:name "Killswitch" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "Select" ;
            lv2:name "Select" ;
        ] ;
    ] .