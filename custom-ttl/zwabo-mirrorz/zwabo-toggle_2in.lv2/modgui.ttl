@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/zwaba/Toggle_2IN>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-toggle2in.html> ;
        modgui:stylesheet <modgui/stylesheet-toggle2in.css> ;
        modgui:screenshot <modgui/screenshot-toggle2in.png> ;
        modgui:thumbnail <modgui/thumbnail-toggle2in.png> ;
        modgui:brand "zwabo" ;
        modgui:label "Toggle_2IN" ;
        modgui:model "boxy-small" ;
        modgui:panel "1-footswitch" ;
        modgui:color "gold" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Toggle" ;
            lv2:name "Toggle" ;
        ] ;
    ] .