@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/zwaba/STUTTER>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-stuttter.html> ;
        modgui:stylesheet <modgui/stylesheet-stuttter.css> ;
        modgui:screenshot <modgui/screenshot-stuttter.png> ;
        modgui:thumbnail <modgui/thumbnail-stuttter.png> ;
        modgui:brand "zwabo" ;
        modgui:label "STUTTTER" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "white" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BPM" ;
            lv2:name "BPM" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "DOUBLE" ;
            lv2:name "DOUBLE" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "Div_L" ;
            lv2:name "Div_L" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "Div_R" ;
            lv2:name "Div_R" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "dry_wet" ;
            lv2:name "dry_wet" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "Filter_R" ;
            lv2:name "Filter_R" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "speed" ;
            lv2:name "speed" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "ENV_ms" ;
            lv2:name "ENV_ms" ;
        ] ;
    ] .